      "src": "/api/finance",
      "dest": "/api/finance.js"
    },
    {
      "src": "/api/cart",
      "dest": "/api/cart.js"
    },
    {
      "src": "/api/orders",
      "dest": "/api/orders.js"
    },
    {
      "src": "/api/sales",
      "dest": "/api/sales.js"
    },
    {
      "src": "/api/reports",
      "dest": "/api/reports.js"
