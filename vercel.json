{
  "version": 2,
  "name": "farmfreshbd-api",
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/auth/(.*)",
      "dest": "/api/auth.js"
    },
    {
      "src": "/api/products/(.*)",
      "dest": "/api/products.js"
    },
    {
      "src": "/api/store_products/(.*)",
      "dest": "/api/store_products.js"
    },
    {
      "src": "/api/farms/(.*)",
      "dest": "/api/farms.js"
    },
    {
      "src": "/api/stats/(.*)",
      "dest": "/api/stats.js"
    },
    {
      "src": "/api/admin/(.*)",
      "dest": "/api/admin.js"
    },
    {
      "src": "/api/auth",
      "dest": "/api/auth.js"
    },
    {
      "src": "/api/products",
      "dest": "/api/products.js"
    },
    {
      "src": "/api/store_products",
      "dest": "/api/store_products.js"
    },
    {
      "src": "/api/farms",
      "dest": "/api/farms.js"
    },
    {
      "src": "/api/stats",
      "dest": "/api/stats.js"
    },
    {
      "src": "/api/admin",
      "dest": "/api/admin.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ],
  "functions": {
    "api/**/*.js": {
      "maxDuration": 30
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}
